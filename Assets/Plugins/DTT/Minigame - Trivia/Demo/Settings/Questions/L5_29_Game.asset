      _isCorrect: 1
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "Th\xEAm b\u0103ng d\xEDnh \u1EDF gi\u1EEFa th\xE1p."
      _isCorrect: 0
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
