    _questionAudio: {fileID: 8300000, guid: ee5566c320efe9b4a964a7ca71b62cf6, type: 3}
    _audioClipFinish: {fileID: 8300000, guid: bee329542e74ed54db1a573411db0dfc, type: 3}
    _matchCorrectAnswers: 0
    _useTitle: 1
    _answers:
    - _body: "Th\xE1p kh\xF4ng rung l\u1EAFc khi \u0111\u1EB7t v\u1EADt n\u1EB7ng."
      _isCorrect: 1
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "Th\xE1p cao nh\u01B0ng kh\xF4ng c\xF3 ph\u1EA7n ch\xE2n \u0111\u1EBF."
      _isCorrect: 0
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "Th\xE1p kh\xF4ng s\u1EED d\u1EE5ng b\u0103ng d\xEDnh."
