    _audioClipFinish: {fileID: 8300000, guid: 234988d82e25acf4e9b8438d6aeacae5, type: 3}
    _matchCorrectAnswers: 0
    _useTitle: 1
    _answers:
    - _body: "S\u1EED d\u1EE5ng c\xE1c l\u1EDBp gi\u1EA5y m\u1ECFng."
      _isCorrect: 0
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "G\u1EA5p gi\u1EA5y th\xE0nh h\xECnh tr\u1EE5."
      _isCorrect: 1
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "X\u1EBFp gi\u1EA5y ph\u1EB3ng ch\u1ED3ng l\xEAn nhau."
      _isCorrect: 0
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
