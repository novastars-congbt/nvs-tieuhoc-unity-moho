      _isCorrect: 0
      _spr: {fileID: 0}
      _audio: {fileID: 0}
      _audioWrong: {fileID: 0}
    - _body: "Kh\xF4ng c\xF3 v\u1EA5n \u0111\u1EC1 g\xEC x\u1EA3y ra"
      _isCorrect: 0
